{
    "manifest_version": 3,
    "default_locale": "en",

    "name": "JollyRogerJargon",
    "description": "Pirate Speak-ify every website",
    "version": "1.0",

    "icons": {
        "48": "images/icon.png",
        "96": "images/icon.png",
        "128": "images/icon.png",
        "256": "images/icon.png",
        "512": "images/icon.png"
    },

    "background": {
        "scripts": [ "background.js" ],
        "type": "module"
    },

    "content_scripts": [{
        "js": [ "content.js" ],
        "matches": [ "<all_urls>" ]
    }],

    "action": {
        "default_popup": "popup.html",
        "default_icon": "images/toolbar-icon.jpg"
    },

    "permissions": [ "activeTab", "storage" ]
}
